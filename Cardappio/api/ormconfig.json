{
   "name": "default",
   "type": "sqlite",
   "database": "./src/database/database.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "./src/models/*.ts"
   ],
   "migrations": [
      "./src/database/migrations/*.ts"
   ],
   "cli": {
      "entitiesDir": "./src/models",
      "migrationsDir": "./src/database/migrations"
   }
}